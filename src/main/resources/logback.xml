<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%-36.36t] %logger{36} - %msg%n</pattern>
    </appender>
    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
    <logger name="com.github.sarxos.webcam" level="OFF"/>
</configuration>